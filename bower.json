{
  "name": "mtg",
  "dependencies": {
    "angular": "~1.3.15",
    "angular-gestures": "~0.3.0",
    "sprintf": "~1.0.2",
    "angular-ui-router": "~0.2.13",
    "font-awesome": "~4.3.0"
  },
  "overrides": {
    "sprintf": {
      "main": {
        "development": [
          "src/sprintf.js",
          "src/angular-sprintf.js"
        ],
        "production": [
          "dist/sprintf.min.js",
          "dist/angular-sprintf.min.js",
          "dist/sprintf.min.map",
          "dist/angular-sprintf.min.map"
        ]
      }
    },
    "angular-gestures": {
      "main": {
        "development": "gestures.js",
        "production": "gestures.min.js"
      }
    },
    "angular-ui-router": {
      "main": {
        "development": "release/angular-ui-router.js",
        "production": "release/angular-ui-router.min.js"
      }
    },
    "angular": {
      "main": {
        "development": "angular.js",
        "production": [
          "angular.min.js",
          "angular.min.js.map"
        ]
      }
    },
    "font-awesome": {
      "main": {
        "development": [
          "css/font-awesome.css",
          "fonts/*"
        ],
        "production": [
          "css/font-awesome.min.css",
          "css/font-awesome.css.map",
          "fonts/*"
        ]
      }
    },
    "hammerjs": {
      "main": {
        "development": "hammer.js",
        "production": [
          "hammer.min.js",
          "hammer.min.map"
        ]
      }
    }
  }
}
